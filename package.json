  },
  "precommit": [
    "check"
  ],
  "dependencies": {
    "JSONStream": "~1.0.4",
    "async": "^1.4.2",
    "bson": "~0.4.11",
    "event-stream": "~3.3.1",
    "lodash.isfunction": "^3.0.6",
    "lodash.reduce": "^3.1.2",
    "moment": "^2.10.6",
    "raf": "^3.1.0"
  },
  "devDependencies": {
    "debug": "^2.2.0",
    "eslint-config-mongodb-js": "^0.1.5",
    "istanbul": "^0.3.19",
    "mocha": "~2.3.2",
    "mongodb-js-precommit": "^0.1.4",
    "pre-commit": "^1.1.1"
  }
}
